	<c:set var="index" value="${ i }"></c:set>
</c:forEach>

<p>Length of String (1) : ${fn:length('하이하이')}</p>
<p>Length of String (2) : ${fn:length(string2)}</p>

<c:set var="length" value="${fn:length('하이하이')}" />

