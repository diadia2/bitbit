package kr.co.bit.reserve.vo;

public class ReserveVO {
	
	private int rsvNo;
	private String memberId;
	private int roomName;
	private int personsNum;
	private int status;
	private String day;
	private int time;
	
	public ReserveVO(){
		
	}

	public int getRsvNo() {
		return rsvNo;
	}

	public void setRsvNo(int rsvNo) {
		this.rsvNo = rsvNo;
	}

	public String getMemberId() {
		return memberId;
	}

	public void setMemberId(String memberId) {
		this.memberId = memberId;
	}

	public int getRoomID() {
		return roomID;
	}

	public void setRoomID(int roomID) {
		this.roomID = roomID;
	}

	public int getPersons_num() {
		return persons_num;
	}

	public void setPersons_num(int persons_num) {
		this.persons_num = persons_num;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getDay() {
		return day;
	}

	public void setDay(String day) {
		this.day = day;
	}

	public String toString() {
		return "ReserveVO [rsvNo=" + rsvNo + ", memberId=" + memberId + ", roomID=" + roomID + ", persons_num="
				+ persons_num + ", status=" + status + ", day=" + day + ", toString()=" + super.toString() + "]";
	}
	
	
}
