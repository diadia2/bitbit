<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%
	request.setCharacterEncoding("utf-8");

	String time = request.getParameter("jTime");
	String id = request.getParameter("getId");
	int roomNo = Integer.parseInt(request.getParameter("jRoomNo"));
	String roomName = null;

	switch (roomNo) {
	case 1:
		roomName = "검찰청";
		break;
	case 2:
		roomName = "관세청";
		break;
	case 3:
		roomName = "대통령";
		break;
	case 4:
		roomName = "국세청";
		break;
	case 5:
		roomName = "교육청";
		break;
	case 6:
		roomName = "경찰청";
		break;
	case 7:
		roomName = "소방청";
		break;
	}
	
	pageContext.setAttribute("time", time);
	pageContext.setAttribute("roomName", roomName);
%>
[
	{ 
		time : '${ time }', 
		roomName : '${ roomName }',
		id : '${ id }'
	}
]